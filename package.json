{
  "name": "@jaunty-script/filter",
  "description": "Validate, transform, and filter data according to a specification",
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "build": "tsc",
    "checkdoc": "checkdoc",
    "cs": "eslint . --ext .ts",
    "test": "jest --ci --coverage",
    "updatedoc": "node updatedoc.js",
    "prepublishOnly": "tsc"
  },
  "keywords": [
    "filter",
    "transform",
    "validate"
  ],
  "author": {
    "name": "JauntyScript Organization",
    "url": "https://github.com/jaunty-script"
  },
  "contributors": [
    {
      "name": "jaunty-script/filter Contributors",
      "url": "https://github.com/jaunty-script/filter/graphs/contributors"
    },
    {
      "name": "traderinteractive/filter-php Contributors",
      "url": "https://github.com/traderinteractive/filter-php/graphs/contributors"
    },
    {
      "name": "traderinteractive/filter-arrays-php Contributors",
      "url": "https://github.com/traderinteractive/filter-arrays-php/graphs/contributors"
    },
    {
      "name": "traderinteractive/filter-bools-php Contributors",
      "url": "https://github.com/traderinteractive/filter-bools-php/graphs/contributors"
    },
    {
      "name": "traderinteractive/filter-dates-php Contributors",
      "url": "https://github.com/traderinteractive/filter-dates-php/graphs/contributors"
    },
    {
      "name": "traderinteractive/filter-floats-php Contributors",
      "url": "https://github.com/traderinteractive/filter-floats-php/graphs/contributors"
    },
    {
      "name": "traderinteractive/filter-ints-php Contributors",
      "url": "https://github.com/traderinteractive/filter-ints-php/graphs/contributors"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaunty-script/filter/issues"
  },
  "homepage": "https://github.com/jaunty-script/filter",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaunty-script/filter"
  },
  "jest": {
    "automock": false,
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "./test/tsconfig.json"
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "resetMocks": true,
    "setupFiles": [
      "core-js"
    ],
    "testRegex": "^.+\\.test\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "devDependencies": {
    "@jaunty-script/checkdoc": "^1.0.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "core-js": "^3.8.3",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-json": "^2.1.2",
    "jest": "^26.6.3",
    "shelljs": "^0.8.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
